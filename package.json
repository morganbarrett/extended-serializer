{
  "name": "extended-serializer",
  "type": "commonjs",
  "version": "1.0.4",
  "description": "",
  "main": "dist/esm/src/index.mjs",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/src/index.d.ts",
        "default": "./dist/esm/src/index.js"
      },
      "default": {
        "types": "./dist/cjs/src/index.d.ts",
        "default": "./dist/cjs/src/index.js"
      }
    }
  },
  "files": [
    "**"
  ],
  "keywords": [
    "extended",
    "serializer",
    "json",
    "parse",
    "stringify"
  ],
  "scripts": {
    "fix": "npm run fix:lint && npm run fix:format",
    "fix:lint": "eslint . --fix",
    "fix:format": "prettier \"*.{js,json,md}\" \"src/**/*.{ts,tsx,md,mdx}\" --write",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc --outDir dist/cjs",
    "build:esm": "tsc --module esnext --outDir dist/esm",
    "test": "vitest run"
  },
  "author": "Morgan Barrett",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/morganbarrett/extended-serializer.git"
  },
  "devDependencies": {
    "@vitest/eslint-plugin": "^1.1.27",
    "@vitest/ui": "^3.0.5",
    "eslint": "^9.19.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-testing-library": "^7.1.1",
    "prettier": "^3.4.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.23.0",
    "vitest": "^3.0.5"
  }
}
